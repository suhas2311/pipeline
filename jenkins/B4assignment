pipeline {
  agent any
  properties([parameters([choice(choices: ['BUILD', 'DEPLOY', 'TEST'], description: 'Select the stage you want to run', name: 'Stage')])]) {
    stages {
      stage ('BUILD') {
        echo 'this is build stage'
        }
      }
      stage ('DEPLOY') {
        echo 'this is deploy stage'
        }
      stage ('TEST') {
        echo 'this is test'
        }
    }  
  }
  
    
